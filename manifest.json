{
    "manifest_version": 3,
    "name": "Assets Watcher",
    "description": "Add unlimited TradingView charts to watchlist.",
    "version": "1.0",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon64.png",
        "128": "icons/icon128.png"
      },
    "permissions": ["tabs"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon64.png",
        "128": "icons/icon128.png"
      }
    },
    "background": {
      "service_worker": "JS/background.js"
    }
  }
  